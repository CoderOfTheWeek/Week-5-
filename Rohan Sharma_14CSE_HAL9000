#include <iostream>

using namespace std;

int prime(int);

int main()
{
    int i;
    for(i=2;i<=10000;i++)
    {
        if(prime(i))
            cout<<i<<"\n";
    }
    return 0;
}

int prime(int n)
{
    int cnt=0,i;
    for(i=2;i<n;i++)
        if(n%i==0)
            cnt++;
    if(cnt==0)
        return 1;
    else
        return 0;
}
